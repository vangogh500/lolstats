{
    "compilerOptions": {
        "outDir": "./dist/js/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "lib": ["es5", "es6", "dom", "esnext"],
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
          "Scenes/*": ["src/Scenes/*"],
          "Components/*": ["src/Components/*"],
          "Services/*": ["src/Services/*"]
        }
    },
    "include": [
        "./src/**/*"
    ]
}
